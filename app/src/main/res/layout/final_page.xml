            android:layout_width="107dp"
            android:layout_height="60dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:text="@string/igor_vishne"
            android:textAppearance="@style/igor_vishne"
            android:gravity="top"
            app:layout_constraintStart_toStartOf="@+id/rectangle_2"
            app:layout_constraintEnd_toEndOf="@+id/rectangle_2" app:layout_constraintTop_toTopOf="@+id/rectangle_2"
            app:layout_constraintBottom_toBottomOf="@+id/rectangle_2" app:layout_constraintHorizontal_bias="0.118"
            app:layout_constraintVertical_bias="0.727"/>
    <View
            android:id="@+id/rectangledt"
            android:layout_width="112dp"
            android:layout_height="42dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:background="@drawable/for_save_page_4"
            app:layout_constraintTop_toTopOf="@+id/rectangle_2"
            app:layout_constraintBottom_toBottomOf="@+id/rectangle_2"
            app:layout_constraintEnd_toEndOf="@+id/rectangle_2"
            app:layout_constraintStart_toStartOf="@+id/rectangle_2" app:layout_constraintHorizontal_bias="0.84"
            app:layout_constraintVertical_bias="0.558"/>
    <TextView
            android:id="@+id/dreamteamtext"
            android:layout_width="88dp"
            android:layout_height="22dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:text="@string/dreamteam"
            android:textAppearance="@style/dreamteam"
            android:gravity="top"
            app:layout_constraintTop_toTopOf="@+id/rectangledt"
            app:layout_constraintBottom_toBottomOf="@+id/rectangledt"
            app:layout_constraintEnd_toEndOf="@+id/rectangledt"
            app:layout_constraintStart_toStartOf="@+id/rectangledt"/>
</androidx.constraintlayout.widget.ConstraintLayout>