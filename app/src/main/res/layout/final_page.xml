            app:layout_constraintStart_toStartOf="@+id/rectangle_1"
            app:layout_constraintEnd_toEndOf="@+id/rectangle_1" app:layout_constraintTop_toTopOf="@+id/rectangle_1"
            app:layout_constraintBottom_toBottomOf="@+id/rectangle_1" app:layout_constraintHorizontal_bias="0.363"
            app:layout_constraintVertical_bias="0.636"/>
    <ImageButton
            android:layout_width="wrap_content"
            android:background="@android:color/transparent"
            android:layout_height="wrap_content" app:srcCompat="@drawable/ic_instagram" android:id="@+id/imageButton4"
            app:layout_constraintStart_toStartOf="@+id/rectangle_1"
            app:layout_constraintEnd_toEndOf="@+id/rectangle_1" app:layout_constraintTop_toTopOf="@+id/rectangle_1"
            app:layout_constraintBottom_toBottomOf="@+id/rectangle_1" app:layout_constraintHorizontal_bias="0.628"
            app:layout_constraintVertical_bias="0.6"/>
    <ImageButton
            android:layout_width="wrap_content"
            android:background="@android:color/transparent"
            android:layout_height="wrap_content" app:srcCompat="@drawable/ic_vk" android:id="@+id/imageButton6"
            app:layout_constraintStart_toStartOf="@+id/rectangle_1"
            app:layout_constraintEnd_toEndOf="@+id/rectangle_1" app:layout_constraintTop_toTopOf="@+id/rectangle_1"
            app:layout_constraintBottom_toBottomOf="@+id/rectangle_1" app:layout_constraintHorizontal_bias="0.876"
            app:layout_constraintVertical_bias="0.7"/>
    <ImageButton
            android:layout_width="55dp"
            android:background="@android:color/transparent"
            android:layout_height="49dp" app:srcCompat="@drawable/ic_download" android:id="@+id/imageButton7"
            app:layout_constraintEnd_toEndOf="@+id/rectangle_1"
            app:layout_constraintStart_toStartOf="@+id/rectangle_1"
            app:layout_constraintTop_toTopOf="@+id/rectangle_1"
            app:layout_constraintBottom_toBottomOf="@+id/rectangle_1" app:layout_constraintHorizontal_bias="0.093"
            app:layout_constraintVertical_bias="0.545"/>
